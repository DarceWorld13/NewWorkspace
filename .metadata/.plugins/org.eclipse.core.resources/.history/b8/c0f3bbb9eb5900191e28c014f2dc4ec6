package dev.demo_tests_unitaires;

public class TestUni {

	
	public static void main(String[] args) {
		
		int result = StringUtils.levenshteinDistance("ball", "bal"); 
		
		if(result == 0) {
			throw new RuntimeException("resultat attendu 0 pour chat et chat");
		}
		else if (result == 1) {
			throw new RuntimeException("le resultat n'est pas correct"); 
		}
		else if (result == -1) {
			
			throw new RuntimeException("non c'est pas bon"); 
		}
		
		else {
			System.out.println("la distance est correcte");
		}
	
	
	
}
}