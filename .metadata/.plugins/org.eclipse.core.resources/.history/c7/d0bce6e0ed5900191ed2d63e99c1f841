package dev.demo_tests_unitaires;

public class TestUni {

	
	public static void main(String[] args) {
		
		int result = StringUtils.levenshteinDistance("ball", null); 
		
		if(result == 0) {
			System.out.println("c'est le résultat attendu");
		}
		if (result == 1) {
			throw new RuntimeException("c'est pas correct"); 
		}
		if (result == -1) {
			
			throw new RuntimeException("non c'est pas bon"); 
		}
		if(result ==2){
			throw new RuntimeException("le résultat n'est pas bon du tout"); 
		}
		if(result == -2){
			throw new RuntimeException("c'est beaucoup trop différent"); 
		}
		
		
	
	
	
}
}