package dev.demo_tests_unitaires;

public class TestUni {

	
	public static void main(String[] args) {
		
		int result = StringUtils.levenshteinDistance("ball", "test"); 
		
		if(result != 0) {
			throw new RuntimeException("ce n'est pas bon"); 
		}
		
		int test1 = StringUtils.levenshteinDistance("chat", "test"); 
		
		if (test1 != 0) {
			throw new RuntimeException("c'est pas correct"); 
		}
		
		int test2 = StringUtils.levenshteinDistance("chatton", "tonchat");
		if (test2 !=0) {
			
			throw new RuntimeException("non c'est pas bon"); 
		}
		
		int test3 = StringUtils.levenshteinDistance("chien", "chienne"); 
		
		if(test3 !=0){
			throw new RuntimeException("le r√©sultat n'est pas bon du tout"); 
		}
		
		
	
	
	
}
}