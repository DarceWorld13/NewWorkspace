package dev.demo_tests_unitaires;

import org.junit.Assert;
import org.junit.Test;






public class TestUni {

	
		//lorsque ça échoue 
		
		@Test
		public void testAdditionner() {
			
			int toto = 12; 
			
			StringUtils calcul = new StringUtils(); 
			calcul.additionner(13, 4); 
			
			Assert.assertEquals(calcul, toto);
			
		}
		
		//lorsque le test passe
		@Test
		public void testSoustraire() {
			
			int tata = 2; 
			StringUtils soutaire = new StringUtils(); 
			soutaire.soustraire(10, 8); 
			
			Assert.assertEquals(tata, soutaire);
		}
		
		
		
		
		
		
	
	
	
}